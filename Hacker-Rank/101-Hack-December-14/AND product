#include <stdio.h>
#include <stdlib.h>

int main(void) {

    unsigned long int T=0;
    long int A=0,B=0,num=0;
    //long int i=0,j=0;
    scanf("%lu",&T);
    while(T--){
        scanf("%ld %ld",&A,&B);
        /*
        num=(A)&(A+1);
        for(i=A+2;i<=B;i++)
            num=(num)&(i);
        */
        
        num=A&B;
        
        printf("%ld\n",num);
    }
    return 0;
}
