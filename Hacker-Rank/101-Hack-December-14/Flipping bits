#include <stdio.h>
#include <math.h>
#include <stdlib.h>

int main(void) {

    unsigned int T=0;
    scanf("%u",&T);
    while(T--){

        unsigned int num=0,res=0,arr[32]={1};
        int i=0,j=0;
        scanf("%u",&num);
        for(i=31;i>=0;i--){
            if(num%2!=0)
                arr[i]=0;
            else
                arr[i]=1;
            num/=2;
        }
        for(i=31,j=0;i>=0;i--,j++)
            res=res+arr[i]*(pow(2,j));
         printf("%u\n",res);
    }
    return 0;
}
