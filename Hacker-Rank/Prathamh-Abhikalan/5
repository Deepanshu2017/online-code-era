#include <stdio.h>
#include <stdlib.h>

int main(void){

	unsigned int t=0;
	scanf("%u",&t);
	while(t--){

		unsigned long int n=0,sum=0;
		scanf("%lu",&n);
		while(n){
			if(n%2!=0)
				sum+=1;
			n/=2;
		}
		printf("%lu\n",sum);
	}
	return 0;
}
