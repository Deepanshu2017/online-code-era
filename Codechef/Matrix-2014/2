#include <stdio.h>
#include <stdlib.h>
 
int main(void){
    unsigned int t=0;
    scanf("%u",&t);
    unsigned long long int arr[55]={0},i=0,j=1;
    arr[0]=1;
    for(i=1;i<52;i++){
        arr[i]=j*2;
        j*=2;
    }
    while(t--){
        unsigned int h=0;
        scanf("%u",&h);
    printf("%llu\n",((arr[h-1])%1000));
    }
    return 0;
} 
