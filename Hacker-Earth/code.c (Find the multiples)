#include<stdio.h>
main(){
long int n,a,i=0;
scanf("%ld",&n);
while(n--){
scanf("%ld",&a);
if(a%3==0)
i++;}
printf("%d",i);
return 0;}

//25.57


main(){long int n,a,i=0;scanf("%ld",&n);while(n--){scanf("%ld",&a);a%3?i:i++;}printf("%d",i);}

