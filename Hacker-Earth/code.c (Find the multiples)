#include<stdio.h>

int main(void){
	long long int n,a,i=0;
	scanf("%lld",&n);
	while(n--){
	scanf("%lld",&a);
	if(a%3==0)
		i+=1;
	}
	printf("%lld",i);
	return 0;
}


