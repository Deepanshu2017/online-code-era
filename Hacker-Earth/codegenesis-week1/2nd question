#include <iostream>
#include <stdio.h>
#include <stdlib.h>
#include <math.h>
using namespace std;

int main(void) {
    unsigned int T = 0 ;
    scanf("%u",&T);
    while(T--) {
    	
    	int m = 0 , n = 0 , x = 0 , y = 0 , a = 0 , b = 0;	
    	scanf("%d %d %d %d %d %d",&a,&b,&m,&n,&x,&y);
    	if(((n - b) * (x - m)) == ((y - n) * (m - a)))
    		printf("YES\n");
    	else
    		printf("NO\n");
    }
    
    return 0;
}
