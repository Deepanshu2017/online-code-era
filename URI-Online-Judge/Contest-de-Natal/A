#include <iostream>
using namespace std;

int main(void) {
	long long int T=0;
	cin>>T;
	while(T--){
		cout<<"Ho";
		if(T)
			cout<<" ";
		else
			cout<<"!";
	}
	cout<<endl;
	return 0;
}
