#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int mat[100][100],i=0,j=0,n=0;
	printf("Enter number of raws\n");
	scanf("%d",&n);
	for(i=0;i<n;i++)
		for(j=0;j<n;j++){
			if(i==j)
				scanf("%d",&mat[i][j]);
			else
				mat[i][j]=0;
		}
	for(i=0;i<n;i++){
		for(j=0;j<n;j++)
			printf("%d ",mat[i][j]);
		printf("\n");
	}
	return 0;
}
